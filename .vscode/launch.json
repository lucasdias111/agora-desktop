{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}"
    },
    {
      "name": "Tauri Dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run tauri dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}